// Demonstrates vector tables.
// Include `support/${PLATFORM}.60p` before this source
// Should print YN

define main routine
  trashes a, x, y, z, n, c, v
{
    ld a, 0
    if z {
        ld a, 89
        call chrout
    } else {
        ld a, 78
        call chrout
    }

    ld a, 1
    if z {
        ld a, 89
        call chrout
    } else {
        ld a, 78
        call chrout
    }
}
